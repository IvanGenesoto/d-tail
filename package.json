{
  "name": "anarch-city",
  "version": "1.0.0",
  "description": "A massively multiplayer persistent online world for people who want to experience a simulated anarchic society.",
  "main": "server/index.js",
  "dependencies": {
    "express": "^4.15.2",
    "performance-now": "^2.1.0",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "htmlhint": "^0.9.13",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "install": "npm install",
    "bundle": "browserify client/index.js -v --transform babelify -o server/public/main.js",
    "start": "npm run bundle && node .",
    "watch:server": "nodemon .",
    "watch:client": "watchify client/index.js -vd --transform babelify -o server/public/main.js",
    "watch": "run-p watch:client watch:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
