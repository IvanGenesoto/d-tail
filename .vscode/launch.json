{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "debug",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: prestart",
      "program": "${workspaceFolder}/server/index.js"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "watch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: watch"
    }
  ]
}